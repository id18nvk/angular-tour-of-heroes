{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recomended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "@angular-eslint"
    ],
    "rules": {
      "indent": [
        "error",
        6
      ]
    }
}
